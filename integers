#include <stdio.h>

int main()
{ int number,number2,number3; 
   printf("Enter three numbers:");
   scanf("%d %d %d",&number, &number2,&number3);
   if
   ((number>number2)&&(number>number3))
   printf("%d is the greatest of three numbers ",number);
   else if 
   ((number2>number)&&(number2>number3))
   printf("%d is the greatest of three numbers ",number2);
   else
   ((number3>number)&&(number3>number2))
  ; printf("%d is the greatest of three numbers",number3);
   return 0;
}
   
